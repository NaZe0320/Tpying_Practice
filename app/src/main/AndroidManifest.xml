<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naze.typing_practice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tpying_Practice">
        <activity android:name="com.naze.typing_practice.StartActivity"></activity>
        <activity android:name="com.naze.typing_practice.RegisterActivity" />
        <activity android:name="com.naze.typing_practice.LoginActivity" />
        <activity android:name="com.naze.typing_practice.activity_long_result" />
        <activity android:name="com.naze.typing_practice.LongPracticeActivity" />
        <activity android:name="com.naze.typing_practice.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.naze.typing_practice.MainActivity" />
    </application>

</manifest>